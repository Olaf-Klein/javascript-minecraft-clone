{
  "name": "javascript-minecraft-clone",
  "version": "0.1.0",
  "description": "A JavaScript-based Minecraft clone with full multiplayer, mod, and plugin support",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "client": "node client/index.js",
    "dev": "concurrently \"npm run start\" \"npm run client:dev\"",
    "client:dev": "vite",
    "client:build": "vite build",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "minecraft",
    "voxel",
    "game",
    "multiplayer",
    "websocket"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "three": "^0.158.0",
    "uuid": "^9.0.1",
    "better-sqlite3": "^9.1.1",
    "noise-simplex": "^1.0.0"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "jest": "^29.7.0"
  }
}
